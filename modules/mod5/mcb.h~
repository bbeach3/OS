#ifndef _MCB_H
#define _MCB_H

#include "mem/heap.h"

typedef struct {
	int alloc; //0 is free, 1 is allocated
	void *address;
	int size;
	void *next;
	void *prev;
} compmcb;

typedef struxt {
	int alloc;
	int size;	
} limitmcb;

struct heap *mcbheap;
void *freelist;
void *alloclist;

int initializeHeap()

#endif
